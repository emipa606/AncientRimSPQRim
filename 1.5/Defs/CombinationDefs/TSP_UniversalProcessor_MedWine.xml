<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!-- Vanilla Beer as example with all fields filled -->
  <!-- Most fields contain default values and can be deleted -->
  <ProcessorFramework.ProcessDef>
    <defName>TSP_AR_UniversalProcessor_NotVFEWine</defName>
    <thingDef>AR_Wine</thingDef>
    <ingredientFilter>
      <thingDefs>
        <li>AR_Mash</li>
      </thingDefs>
    </ingredientFilter>
    <!-- <maxCapacity>75</maxCapacity> -->
    <usesTemperature>true</usesTemperature>
    <temperatureSafe>0.5~30</temperatureSafe>
    <temperatureIdeal>14.~25.0</temperatureIdeal>
    <ruinedPerDegreePerHour>0.5</ruinedPerDegreePerHour>
    <speedBelowSafe>0.0</speedBelowSafe>
    <speedAboveSafe>1.0</speedAboveSafe>
    <processDays>8</processDays>
    <!-- in days, a day is 60,000 ticks -->
    <capacityFactor>1.0</capacityFactor>
    <!-- do not use too many decimals e.g 0.333 is fine for 1/3 -->
    <efficiency>1.0</efficiency>
    <sunFactor>1.0~1.0</sunFactor>
    <rainFactor>1.0~1.0</rainFactor>
    <snowFactor>0.95~1.0</snowFactor>
    <windFactor>1.0~1.0</windFactor>
    <unpoweredFactor>0.0</unpoweredFactor>
    <unfueledFactor>0.0</unfueledFactor>
    <powerUseFactor>1.0</powerUseFactor>
    <fuelUseFactor>1.0</fuelUseFactor>
    <filledGraphicSuffix>
    </filledGraphicSuffix>
    <usesQuality>false</usesQuality>
    <qualityDays>(1, 2, 3, 4, 5, 6, 7)</qualityDays>
    <color>(64, 0, 128)</color>
    <!-- 0-255 or 0.0-1.0 -->
    <customLabel>
    </customLabel>
    <destroyChance>0.0</destroyChance>
    <bonusOutputs>
      <!-- <thingDef>(1.0, 1.0)</thingDef> -->
    </bonusOutputs>
  </ProcessorFramework.ProcessDef>
</Defs>
