  <RecipeDef ParentName="CookMealBase">
    <defName>AR_FryMeat</defName>
    <label>fry meat in olive oil</label>
    <workAmount>300</workAmount>
    <description>Cooks a relatively simple meal by frying raw meat in fatty olive oil.</description>
    <descriptionHyperlinks>
      <thingDef>AR_OliveOil</thingDef>
      <thingDef>AR_RawOlives</thingDef>
      <thingDef>AR_Salad</thingDef>
      <thingDef>AR_FryedEgg</thingDef>
    </descriptionHyperlinks>
    <jobString>frying meat in olive oil</jobString>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>MeatRaw</li>
          </categories>
        </filter>
        <count>0.5</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>AR_OliveOil</li>
			<li MayRequire="sumika.nanohana">PlantOil</li>
          </thingDefs>
        </filter>
        <count>0.21</count>
        <!-- Three units -->
      </li>
    </ingredients>
    <products>
      <AR_FryedMeat>1</AR_FryedMeat>
    </products>
    <fixedIngredientFilter Inherit="False">
      <categories>
        <li>MeatRaw</li>
      </categories>
      <thingDefs>
        <li>AR_OliveOil</li>
		<li MayRequire="sumika.nanohana">PlantOil</li>
      </thingDefs>
    </fixedIngredientFilter>
    <skillRequirements>
      <Cooking>1</Cooking>
    </skillRequirements>
    <researchPrerequisite>AR_Project_Mediterranean_Culture</researchPrerequisite>
    <recipeUsers>
      <li>ElectricStove</li>
      <li>FueledStove</li>
      <li MayRequire="VanillaExpanded.VCookE">VCE_DeepFrier</li>
      <li MayRequire="TSP.Isengriff.Storytime">GnomeTable</li>
      <li MayRequire="morishep.tarojun.onioftherim">OotR_GroundOven</li>
	  <li MayRequire="nstr.malay.themed.expansion">NSTRDapur</li>
	  <li MayRequire="nstr.malay.themed.expansion">NSTRElectricDapur</li>
    </recipeUsers>
  </RecipeDef>
