  <ThingDef ParentName="FurnitureBase">
    <defName>AR_OilLamp</defName>
    <label>oil lamp</label>
    <uiIconPath>Icon/OilLamp</uiIconPath>
    <clearBuildingArea>false</clearBuildingArea>
    <building>
      <isEdifice>false</isEdifice>
      <canBuildNonEdificesUnder>true</canBuildNonEdificesUnder>
    </building>
    <researchPrerequisites>
      <li>AR_Project_OliveTree</li>
    </researchPrerequisites>
    <description>A simple Lamp. Burns oil to work.</description>
    <category>Building</category>
    <thingClass>Building</thingClass>
    <graphicData>
      <texPath>Buildings/Furniture/OilLamp</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawRotated>false</drawRotated>
      <drawSize>2</drawSize>
    </graphicData>
    <rotatable>false</rotatable>
    <altitudeLayer>MoteOverhead</altitudeLayer>
    <passability>Standable</passability>
    <pathCost>8</pathCost>
    <tickerType>Normal</tickerType>
    <drawerType>RealtimeOnly</drawerType>
    <fillPercent>0.15</fillPercent>
    <statBases>
      <MaxHitPoints>80</MaxHitPoints>
      <WorkToBuild>200</WorkToBuild>
      <Flammability>1</Flammability>
      <Beauty>5</Beauty>
      <Mass>0.25</Mass>
    </statBases>
    <selectable>true</selectable>
    <stuffCategories>
      <li>Metallic</li>
      <li>Stony</li>
      <!-- Mod support -->
      <li MayRequire="Dubwise.Rimefeller">Plastic</li>
      <li MayRequire="Dubwise.Rimefeller">Composite</li>
      <li MayRequire="sarg.alphabiomes">AB_Exotic</li>
      <li MayRequire="sarg.alphabiomes">AB_ExoticMetals</li>
    </stuffCategories>
    <costStuffCount>30</costStuffCount>
    <costList>
      <AR_OliveOil>10</AR_OliveOil>
    </costList>
    <leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
    <placeWorkers>
      <li>PlaceWorker_Heater</li>
    </placeWorkers>
    <drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
    <comps>
      <li Class="CompProperties_Refuelable">
        <fuelConsumptionRate>1</fuelConsumptionRate>
        <fuelCapacity>40</fuelCapacity>
        <fuelConsumptionPerTickInRain>0.01</fuelConsumptionPerTickInRain>
        <fuelFilter>
          <thingDefs>
            <li>AR_OliveOil</li>
            <li MayRequire="sumika.nanohana">PlantOil</li>
			<li>Chemfuel</li>
          </thingDefs>
        </fuelFilter>
        <initialFuelPercent>1</initialFuelPercent>
        <destroyOnNoFuel>false</destroyOnNoFuel>
      </li>
      <li Class="CompProperties_Glower">
        <glowRadius>11</glowRadius>
        <glowColor>(252,187,113,0)</glowColor>
      </li>
      <li Class="CompProperties_HeatPusher">
        <heatPerSecond>0.5</heatPerSecond>
        <heatPushMaxTemperature>23</heatPushMaxTemperature>
      </li>
      <li Class="CompProperties_FireOverlay">
        <fireSize>0.2</fireSize>
        <offset>(-0.3,0,0.4)</offset>
      </li>
      <li Class="CompProperties_Forbiddable"></li>
